{
    "tags": {
        "allowUnknownTags": true
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "outputSourceFiles": true,
        "outputSourcePath": false,
        "linenums": true
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": false
    },
    "opts": {
        "encoding": "utf8",
        "recurse": true,
        "private": false,
        "lenient": true
    }
}